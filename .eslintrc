{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-mixed-spaces-and-tabs": 0,
    "react/jsx-key": 1,
    "no-console": 0,
    "no-unused-vars": 0
  },
  "plugins": [
    "react"
  ]
}